  "bugs": {
    "url": "https://github.com/pixeldesu/misairu/issues"
  },
  "homepage": "https://github.com/pixeldesu/misairu#readme",
  "devDependencies": {
    "microbundle": "^0.13.3",
    "typescript": "^4.4.2"
  }
}
